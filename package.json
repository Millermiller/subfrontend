{
  "name": "subfrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build local": "vue-cli-service build --mode development",
    "build prod": "vue-cli-service build",
    "serve": "vue-cli-service serve --mode development --host learn.scandinaver.local",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@types/reflect-metadata": "^0.1.0",
    "axios": "^0.19.2",
    "class-transformer": "^0.2.3",
    "core-js": "^3.6.5",
    "element-ui": "^2.13.1",
    "pug": "^2.0.4",
    "pug-load": "^2.0.12",
    "pug-plain-loader": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "smooth-scrollbar": "^8.5.2",
    "swiper": "^5.3.8",
    "typedi": "^0.8.0",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^3.1.3",
    "vue-class-component": "^7.2.3",
    "vue-cookies": "^1.7.0",
    "vue-custom-scrollbar": "^1.2.0",
    "vue-drag-drop": "^1.1.4",
    "vue-i18n": "^8.17.4",
    "vue-meta": "^2.3.3",
    "vue-progressbar": "^0.7.5",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.1.6",
    "vue-rx": "^6.2.0",
    "vue-smooth-scrollbar": "^0.1.2",
    "vue-template-loader": "^1.1.0",
    "vue-typedi": "^1.1.1",
    "vuex": "^3.3.0",
    "vuex-smart-module": "^0.3.2"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@types/swiper": "^5.3.0",
    "@types/vue-i18n": "^7.0.0",
    "@types/webpack": "^4.4.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-e2e-cypress": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-mocha": "^4.3.1",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "canvas": "^2.6.1",
    "chai": "^4.1.2",
    "cypress": "^4.9.0",
    "cypress-intellij-reporter": "0.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.3",
    "node-sass": "^4.14.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.0",
    "typescript": "~3.6.3",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "rules": {
      "object-curly-newline": "off",
      "no-param-reassign": "off",
      "class-methods-use-this": "off",
      "lines-between-class-members": "off",
      "import/no-unresolved": "off",
      "no-shadow": "off",
      "max-len": "off",
      "no-nested-ternary": "off",
      "semi": "off",
      "import/named": "off",
      "import/newline-after-import": "off",
      "import/prefer-default-export": "off",
      "import/no-cycle": "off",
      "camelcase": "off",
      "no-plusplus": "off",
      "no-console": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
